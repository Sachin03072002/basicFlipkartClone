<section>
  <h4>
      Posts
  </h4>
  <% if(locals.user){ %>
  <form action="/posts/create" id="" method="Post">
      <textarea name="content" id="" cols="30" rows="3" placeholder="Type Here..."></textarea>
      <input type="submit" value="Post">
  </form>
  <%} %>
  <div>
    <ul>
      <% for(post of posts){ %>
        <li>
          <p>
          <%= post.content %>
          <br>
          <small>
            <%= post.user.name %>
          </small>
        </p>
        </li>
      
      
      
      
      
      <% } %>
    
    
    
    </ul>
  </div>
</section>